#List declared in [] bracket also values can be changed not immuteable
values = [1, 2, 3, "Rajesh", 4, 5]
#List is a data type that allows multiple values and can be different data types
print(values[0]) #1
print(values[3]) #Rajesh
print(values[5]) #5

#to print the last value
print(values[-1])

#to print the sequence from index to index
print(values[1:3]) #2 3 prints index 1 and 2

# to insert any value in the list
values.insert(4,"kumar")
print(values)

#to add new value at the end
values.append("End")
print(values)

values.append("last")
print(values)

#to update the value
values[3] = "RAJESH"
print(values)

#delete the values
del values[4]
print(values)
